{
  "name": "trekanic",
  "version": "1.0.0",
  "description": "The appointment scheduler",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build --verbose",
    "lint": "eslint \"packages/**/*.ts\" --fix",
    "start": "ts-node --project tsconfig.json src/index.ts",
    "prepare": "husky-init"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@monorepo/ui": "^1.0.0",
    "@monorepo/utils": "^1.0.0",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "main": "index.js",
